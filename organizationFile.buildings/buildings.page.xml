<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>buildings</object-id>
    <layout>n2o/layout/single</layout>
    <name>Здания</name>

    <regions>
        <region place="single" type="pills">

            <container id="constructionCost">
                <wgt:table ref-id="constructionCostList"/>
            </container>

            <container id="etageLevels">
                <wgt:table ref-id="etageLevelsList"/>
            </container>

            <container id="facilities">
                <wgt:table ref-id="facilitiesList"/>
            </container>

            <container id="facilitiesProfiles">
                <wgt:table ref-id="facilitiesProfilesList"/>
            </container>

            <container id="facilitiesStatuses">
                <wgt:table ref-id="facilitiesStatusesList"/>
            </container>


        </region>
    </regions>
</page>
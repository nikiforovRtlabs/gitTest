<?xml version='1.0' encoding='UTF-8'?>
<object xmlns:n2o="http://n2oapp.net/framework/config/schema/action-java-method-1.0"
        xmlns="http://n2oapp.net/framework/config/schema/object-1.0">

    <actions>
        <action id="set">
            <invocation>
                <n2o:java-method bean="defaultCurrentContext" name="set">
                    <n2o:arguments>
                        <n2o:argument
                                class="net.n2oapp.criteria.dataset.DataSet"
                                name="context"/>
                    </n2o:arguments>
                </n2o:java-method>
            </invocation>
            <in-parameters>
                <param name="post.id" mapping="[0]['post.id']"/>
                <param name="cab.id" mapping="[0]['cab.id']"/>
                <param name="dep.id" mapping="[0]['dep.id']"/>
                <param name="org.id" mapping="[0]['org.id']"/>
                <param name="pos.id" mapping="[0]['pos.id']"/>
            </in-parameters>
        </action>
    </actions>

</object>
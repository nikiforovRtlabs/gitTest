<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>secDebug</object-id>
    <name>secDebug</name>
    <execution>
        <n2o:java-criteria bean-name="accessInfoService"
                           criteria-class="ru.kirkazan.rmis.app.security.debug.AccessInfoCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <display/>
        </field>

        <field>
            <id>info</id>
            <domain>object</domain>
            <filters>
                <filter type="eq" filter-field="info"/>
            </filters>
        </field>

        <field>
            <id>type</id>
            <domain>string</domain>
            <filters>
                <filter type="eq" filter-field="type"/>
            </filters>
        </field>

        <field>
            <id>allowed</id>
            <domain>boolean</domain>
            <display/>
        </field>


        <field>
            <id>detail</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>tech</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>isError</id>
            <domain>boolean</domain>
            <display/>
        </field>

        <field>
            <id>error.msg</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>error.det</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>error.tech</id>
            <domain>string</domain>
            <display/>
        </field>


    </fields>
</query>

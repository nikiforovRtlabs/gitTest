    <object-id>diagnosisClinicalForm</object-id>
    <name>Клиническая форма диагноза</name>
    <layout>n2o/layout/top-bottom</layout>
    <regions>
        <region place="top">
            <container id="clinicalForm" place="top">
                <wgt:tree>
                    <wgt:name>Ajax</wgt:name>
                    <wgt:query-id>diagnosisClinicalForm</wgt:query-id>

<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">

    <object-id>itemPage</object-id>
    <execution>
        <n2o:java-criteria bean-name="itemPageService"
                           criteria-class="net.n2oapp.criteria.dataset.DynamicCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <name>Наименование</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>space.id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>page.id</id>
            <name>Страница</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>url</id>
            <name>Ссылка</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>pageId</id>
            <name>Страница</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>out</id>
            <domain>boolean</domain>
            <sorting unavailable="true"/>
        </field>
    </fields>
</query>
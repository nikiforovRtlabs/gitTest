<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">

    <object-id>module</object-id>
    <execution>
        <n2o:java-criteria bean-name="moduleService"
                           criteria-class="net.n2oapp.criteria.dataset.DynamicCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <name>Имя</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>code</id>
            <name>Код</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>type</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>mainPage.id</id>
            <name>Главная страница</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>url</id>
            <name>URL</name>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>out</id>
            <name>Внешний модуль</name>
            <domain>boolean</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>group.id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>fullName</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>hasChildren</id>
            <domain>boolean</domain>
            <sorting unavailable="true"/>
        </field>
    </fields>
</query>
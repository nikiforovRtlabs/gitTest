<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>epicrisis_report</object-id>
    <name>hosp_reports</name>
    <execution>
        <n2o:sql>
            <n2o:items-query>select :select
                from cmn_report a
                where :where
                order by :order
            </n2o:items-query>
            <n2o:count-query>select count(a)
                from cmn_report a
                where :where
            </n2o:count-query>
            <n2o:alias>a</n2o:alias>
        </n2o:sql>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <expression>a.id</expression>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <expression>a.name</expression>
            <filters>
                <filter type="eq" filter-field="name">upper(:expression) like '%'||upper(:name)||'%'</filter>
            </filters>
            <display/>
        </field>

    </fields>
</query>

<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>epicrisis_section</object-id>
    <name>epicrisis_section</name>
    <execution>
        <n2o:java-criteria bean-name="epicrisisSectionService"
                           criteria-class="ru.kirkazan.rmis.app.ehr.api.criteria.EpicrisisSectionCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <expression>id</expression>
            <display/>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <name>Раздел</name>
            <display/>
        </field>
        <field>
            <id>isInclude</id>
            <domain>boolean</domain>
            <expression>include</expression>
            <name>Включить в эпикриз</name>
            <display/>
        </field>
        <field>
            <id>isAutomatic</id>
            <domain>boolean</domain>
            <expression>automatic</expression>
            <name>Собирать автоматически</name>
            <display/>
        </field>
    </fields>
</query>

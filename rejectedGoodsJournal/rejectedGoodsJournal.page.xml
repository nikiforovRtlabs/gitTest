<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>rejectedGoodsJournal</object-id>
    <layout>n2o/layout/single</layout>
    <name>Журнал забракованных товаров</name>

    <regions>
        <region place="single" type="none">
            <container id="main">
                <wgt:table>
                    <wgt:query-id>rejectedGoodsJournal</wgt:query-id>
                    <wgt:size>30</wgt:size>
                    <wgt:columns>
                        <wgt:column column-field-id="holdModif.name" width="30%"/>
                        <wgt:column column-field-id="series"/>
                        <wgt:column column-field-id="expireDate"/>
                        <wgt:column column-field-id="producer.name" width="25%"/>
                        <wgt:column column-field-id="country.name" width="10%"/>
                        <wgt:column column-field-id="operationType.name" width="10%"/>
                        <wgt:column column-field-id="letter.name" width="10%"/>
                    </wgt:columns>
                </wgt:table>
            </container>
        </region>
    </regions>

</page>

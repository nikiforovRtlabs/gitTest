<role xmlns="http://atria.cz/sec/config/schema/role-1.0">
    <name>Администратор безопасности в МО</name>
    <group>Управление доступом</group>
    <section id="domainObjects">
        <element id="secUser" access="allowed">
            <permission id="accessKeyRegion" access="denied"/>
        </element>
        <element id="secRole">
            <permission id="read"/>
            <permission id="createMO"/>
            <permission id="updateMO"/>
            <permission id="deleteMO"/>
        </element>
        <element id="secGroup"/>
    </section>
</role>
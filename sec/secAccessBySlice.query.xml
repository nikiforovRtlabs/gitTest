<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:java="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <object-id>secAccessBySlice</object-id>
    <execution>
        <java:java-criteria bean-name="accessBySliceService"
                            criteria-class="ru.kirkazan.rmis.app.security.admin.api.AccessBySliceCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
        </field>

        <field>
            <id>name</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>


        <field>
            <id>groupId</id>
            <domain>integer</domain>
            <sorting unavailable="true"/>
        </field>

        <field>
            <id>accessKeyId</id>
            <domain>integer</domain>
            <sorting unavailable="true"/>
            <display unavailable="true"/>
        </field>

        <field>
            <id>orgId</id>
            <domain>integer</domain>
            <sorting unavailable="true"/>
            <display unavailable="true"/>
        </field>

        <field>
            <id>slicerId</id>
            <domain>string</domain>
            <sorting unavailable="true"/>
        </field>

    </fields>
</query>
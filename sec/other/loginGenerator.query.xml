<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:java="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <object-id>loginGenerator</object-id>
    <execution>
        <java:java-criteria bean-name="loginGenerator"
                            criteria-class="net.n2oapp.criteria.dataset.DynamicCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>login</id>
            <domain>String</domain>
            <display>['login']</display>
        </field>
        <field>
            <id>name</id>
            <domain>String</domain>
            <display unavailable="true"/>
        </field>

    </fields>
</query>
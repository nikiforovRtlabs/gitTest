<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>patSigDiagnosest</object-id>
    <layout>n2o/layout/single</layout>

    <modal-width>950px</modal-width>
    <regions>
        <region place="single" type="none">
            <container id = "main">
                <wgt:table>
                    <wgt:query-id>patSigDiagnosest</wgt:query-id>
                    <wgt:name>Значимые диагнозы: история</wgt:name>
                    <wgt:size>20</wgt:size>
                    <wgt:columns>
                        <wgt:column column-field-id="diagnosName" width="350px"/>
                        <wgt:column column-field-id="reg_name" width="150"/>
                        <wgt:column column-field-id="regDate" width="100px"/>
                        <wgt:column column-field-id="unregDate" width="100px"/>
                        <wgt:column column-field-id="reason" width="150"/>
                    </wgt:columns>
                    <wgt:action-menu>
                        <wgt:menu-item id="hiden"  visible="false">
                            <wgt:a href="null"/>
                        </wgt:menu-item>
                    </wgt:action-menu>

                </wgt:table>
            </container>
        </region>
    </regions>

</page>

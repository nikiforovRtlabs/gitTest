    <ctrl:classifier id="administrationRoute" label="Способ введения" search-are-you-type="true" required="true"
                     control-style="width: 700px;" label-style="width: 130px;">
        <ctrl:query query-id="hosp_administrationRoute" value-field-id="id" label-field-id="name">
            <!--<ctrl:pre-filters>-->
                <!--<ctrl:pre-filter field-id="formTypeId" ref="holdings.0.holding.form_type_id"/>-->
            <!--</ctrl:pre-filters>-->
        </ctrl:query>
        <ctrl:set-value if="typeof id == 'undefined'">
            <ctrl:then>
                <ctrl:value field-id="id">4</ctrl:value>
                <ctrl:value field-id="name">Внутривенно</ctrl:value>
            </ctrl:then>
        </ctrl:set-value>
    </ctrl:classifier>

    <ctrl:classifier id="prescriptionPeriodicity" label="Периодичность" search-are-you-type="true" required="true"
                     control-style="width: 700px;" label-style="width: 130px;" visible="false">

        <ctrl:default-model>
            <ctrl:value field-id="id">7</ctrl:value>
        </ctrl:default-model>

    </ctrl:classifier>

    <ctrl:classifier id="visitTerm" label="Условия" search-are-you-type="true"
                     control-style="width: 700px;" label-style="width: 130px;" visible="false">
        <ctrl:query query-id="hosp_visitTerm" value-field-id="id" label-field-id="name"/>
    </ctrl:classifier>

    <ctrl:classifier id="status" label="Статус" search-are-you-type="true"
                     control-style="width: 700px;" label-style="width: 130px;" visible="false">
        <ctrl:query query-id="prescriptionStatus" value-field-id="id" label-field-id="name"/>
        <ctrl:default-model>
            <ctrl:value field-id="id">2</ctrl:value>
            <ctrl:value field-id="name">Запланировано</ctrl:value>
        </ctrl:default-model>
    </ctrl:classifier>


    <ctrl:checkbox id="isOperation" label="Назначение операции"

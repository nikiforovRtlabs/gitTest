<?xml version='1.0' encoding='UTF-8'?>
<fs:field-set xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
              xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0">
    <fs:row>
        <ctrl:output-text id="severity" label="Состояние пациента:"/>
        <ctrl:output-text id="consciousness" label="Сознание:"/>
        <ctrl:output-text id="pressure" label="AD мм рт.ст.:"/>
        <ctrl:output-text id="pulseRate" label="Пульс:"/>
        <ctrl:output-text id="breathRate" label="Частота дыхания:"
                          dependency-condition="breathRate!=null"/>
        <ctrl:output-text id="temperature" label="Т:"/>

        <ctrl:output-text id="anesthesiaType" label="Вид анестезии:"/>
        <ctrl:output-text id="anesthetic" label="Препарат анестезии:"
                          dependency-condition="anesthetic!=null"/>
        <ctrl:output-text id="beginDateTime" label="Начало анестезии:" control-style="width=200px"/>
        <ctrl:output-text id="anesthesiaRisk" label="Риск анестезии:"
                          dependency-condition="anesthesiaRisk!=null"/>

        <ctrl:output-text id="premedication" label="Премедикация:" control-style="width=500px"
                          dependency-condition="premedication!=null"/>

        <ctrl:output-text id="anesthesiolog" label="Анестезиолог:"/>
        <ctrl:output-text id="anesthesist" label="Анестезист:"/>
        <ctrl:output-text id="operationJournalId" visible="false"/>


    </fs:row>
</fs:field-set>



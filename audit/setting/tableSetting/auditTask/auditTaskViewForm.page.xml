<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>auditTask</object-id>
    <layout>n2o/layout/single</layout>

    <containers>
        <container id="main" place="single">
            <wgt:form>
                <wgt:query-id>auditTask</wgt:query-id>
                <wgt:name>Просмотр записи</wgt:name>
                <fs:field-set>
                    <ctrl:input-text id="from_partition_period_name" readonly="true"/>
                    <ctrl:input-text id="to_partition_period_name" readonly="true"/>
                    <ctrl:input-text id="status_name" readonly="true"/>
                    <ctrl:input-text id="mode_type_name" readonly="true"/>
                    <ctrl:input-text id="created_at" readonly="true"/>
                    <ctrl:input-text id="created_by" readonly="true"/>
                    <ctrl:input-text id="updated_at" readonly="true"/>
                    <ctrl:input-text id="canceled_at" readonly="true"/>
                    <ctrl:input-text id="canceled_by" readonly="true"/>
                    <ctrl:text-area id="error_message" rows="5" readonly="true"/>
                </fs:field-set>
            </wgt:form>
        </container>
    </containers>
</page>

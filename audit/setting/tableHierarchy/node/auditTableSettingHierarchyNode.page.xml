<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>auditSetting</object-id>
    <layout>n2o/layout/single</layout>

    <regions>
        <region place="single" type="none">
            <container id="main">
                <wgt:form>
                    <wgt:query-id>auditTableSettingHierarchyNode</wgt:query-id>
                    <fs:field-set>
                        <ctrl:hidden id="parent_id"/>
                        <ctrl:input-text id="name" label="Наименование" required="true"/>
                        <ctrl:input-text domain="integer" id="position" label="Позиция"/>
                    </fs:field-set>
                </wgt:form>
            </container>
        </region>
    </regions>
</page>

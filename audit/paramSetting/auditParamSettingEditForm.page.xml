<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>auditParamSetting</object-id>
    <layout>n2o/layout/single</layout>

    <regions>
        <region place="single" type="tabs">
            <container id="main">
                <wgt:form>
                    <wgt:query-id>auditParamSetting</wgt:query-id>
                    <wgt:name>Редактировать настройку</wgt:name>
                    <fs:field-set>
                        <ctrl:input-text id="name"/>
                        <ctrl:input-text id="code" readonly="true"/>
                        <ctrl:input-text id="value" domain="integer" min="1000" max="100000000"/>
                    </fs:field-set>
                </wgt:form>
            </container>
        </region>
    </regions>
</page>

<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>metaData</object-id>
    <name>Каталог метаданных</name>
    <execution>
        <n2o:java-criteria bean-name="metaDataCatalogCriteriaService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.config.criteria.catalog.MetaDataCatalogCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
        </field>
        <field>
            <id>name</id>
            <domain>String</domain>
            <name>Наименование</name>
        </field>
        <field>
            <id>catalog.id</id>
            <domain>string</domain>
        </field>
    </fields>
</query>
<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <!--<id>query</id>-->
    <object-id>query2</object-id>
    <execution>
        <n2o:java-criteria bean-name="queryService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.object.criteria.QueryCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <name>Код</name>
        </field>
        <field>
            <id>id_2</id>
            <domain>String</domain>
            <name>Идентификатор</name>
        </field>
        <field>
            <id>invalidMessage</id>
            <domain>String</domain>
            <name>Ошибка валидации</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>invalid</id>
            <domain>boolean</domain>
            <name></name>
        </field>

        <field>
            <id>object.id</id>
            <domain>String</domain>
        </field>
        <field>
            <id>object.name</id>
            <domain>String</domain>
            <name>Объект</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <domain>String</domain>
            <name>Наименование</name>
            <search>name_compiled</search>
        </field>
        <field>
            <id>name_compiled</id>
            <domain>String</domain>
            <name>Наименование</name>
        </field>
        <field>
            <id>sql.queryItems</id>
            <domain>String</domain>
            <name>SQL запрос за данными</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>sql.queryCount</id>
            <domain>String</domain>
            <name>SQL запрос за количеством</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>sql.alias</id>
            <domain>String</domain>
            <name>Алиас</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>sql.dataSource</id>
            <domain>String</domain>
            <name>Источник Базы Данных (jndi)</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>

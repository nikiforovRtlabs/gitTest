<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>report2</object-id>
    <execution xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
        <n2o:java-criteria bean-name="templateCollectionService"
                           criteria-class="ru.kirkazan.rmis.app.report.constructor.template.TemplateCriteria">
        </n2o:java-criteria>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
        </field>
        <field>
            <id>name</id>
            <name>Имя файла</name>
            <domain>string</domain>
        </field>
        <field>
            <id>path</id>
            <name>Абсолютный путь к файлу</name>
            <domain>string</domain>
        </field>
        <field>
            <id>extensions</id>
            <domain>string[]</domain>
            <display unavailable="true"/>
        </field>
    </fields>
</query>


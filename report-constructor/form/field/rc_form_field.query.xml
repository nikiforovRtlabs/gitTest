<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>rc_form_field</object-id>
    <execution xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
        <n2o:java-criteria bean-name="formFieldCollectionService"
                           criteria-class="ru.kirkazan.rmis.app.report.constructor.form.field.FieldCriteria">
        </n2o:java-criteria>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
        </field>
        <field>
            <id>fieldId</id>
            <name>Параметр отчета</name>
            <domain>string</domain>
        </field>
        <field>
            <id>formId</id>
            <domain>string</domain>
        </field>
        <field>
            <id>label</id>
            <name>Наименование на форме</name>
            <domain>string</domain>
        </field>
        <field>
            <id>required</id>
            <name>Обязательность</name>
            <domain>boolean</domain>
        </field>
        <field>
            <id>visible</id>
            <name>Видимость</name>
            <domain>boolean</domain>
        </field>
        <field>
            <id>type.id</id>
            <name>Тип</name>
            <domain>string</domain>
            <display>typeId</display>
        </field>
        <field>
            <id>typeName</id>
            <name>Тип</name>
            <domain>string</domain>
        </field>
        <field>
            <id>finder.id</id>
            <name>Выборка</name>
            <domain>string</domain>
            <display>classifierQueryId</display>
        </field>
        <field>
            <id>reportQuery.id</id>
            <domain>string</domain>
            <expression>reportQueryId</expression>
        </field>
        <field>
            <id>reportQueryField.id</id>
            <domain>string</domain>
            <expression>reportQueryFieldId</expression>
        </field>
        <field>
            <id>reportQueryField.name</id>
            <domain>string</domain>
            <name>Наименование поля выборки</name>
            <expression>reportQueryFieldLabel</expression>
        </field>
        <field>
            <id>defaultValue</id>
            <domain>string</domain>
            <name>Значение по умолчанию</name>
        </field>
    </fields>
</query>


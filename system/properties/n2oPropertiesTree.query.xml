<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>n2oObject</object-id>
    <name>Properties</name>
    <execution>
        <n2o:java-criteria bean-name="propertiesTreeService"
                           criteria-class="net.n2oapp.criteria.api.Criteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>name</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>parent</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>hasChildren</id>
            <domain>string</domain>
            <display/>
        </field>

        <field>
            <id>canResolved</id>
            <domain>string</domain>
            <display/>
        </field>


    </fields>
</query>

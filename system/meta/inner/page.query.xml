            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>object.id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>object.name</id>
            <domain>String</domain>
            <name>Объект</name>
            <sorting unavailable="true"/>

            <search unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <name>Наименование</name>
            <domain>string</domain>
            <search unavailable="true"/>
        </field>
        <field>
            <id>nameCompiled</id>
            <name>Наименование</name>
            <domain>string</domain>
        </field>
        <field>
            <id>layout.id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>layout.name</id>
            <domain>String</domain>
            <name>Макет</name>
            <sorting unavailable="true"/>
        </field>
    </fields>
</query>
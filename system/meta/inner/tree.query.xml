            <id>widgetId</id>
            <domain>String</domain>
        </field>
        <field>
            <id>queryId</id>
            <domain>String</domain>
        </field>
        <field>
            <id>parentFieldId</id>
            <domain>String</domain>
        </field>
        <field>
            <id>labelFieldId</id>
            <domain>String</domain>
        </field>
        <field>
            <id>hasChildrenFieldId</id>
            <domain>String</domain>
        </field>
        <field>
            <id>type</id>
            <domain>integer</domain>
        </field>
        <field>
            <id>pageId</id>
            <domain>string</domain>
        </field>
    </fields>

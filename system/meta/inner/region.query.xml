<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <id>region</id>
    <object-id>region</object-id>
    <execution>
        <n2o:java-criteria bean-name="regionService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.object.criteria.RegionCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>localId</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>layout.id</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>layout.name</id>
            <domain>String</domain>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <domain>String</domain>
        </field>
    </fields>
</query>
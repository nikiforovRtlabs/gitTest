<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <object-id>object</object-id>
    <execution>
        <n2o:java-criteria bean-name="objectService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.object.criteria.ObjectCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <name>Код</name>
        </field>
        <field>
            <id>invalidMessage</id>
            <domain>String</domain>
            <name>Ошибка валидации</name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>invalid</id>
            <domain>boolean</domain>
            <name></name>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <name>Наименование</name>
        </field>
        <field>
            <id>name_compiled</id>
            <domain>string</domain>
            <name>Наименование</name>
            <search>name</search>
        </field>
        <field>
            <id>catalog.id</id>
            <domain>String</domain>
            <name>Каталог</name>
        </field>
        <field>
            <id>catalog.name</id>
            <domain>String</domain>
            <name>Каталог</name>
            <search unavailable="true"/>
        </field>
        <field>
            <id>isSystem</id>
            <domain>Boolean</domain>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>hasAncestor</id>
            <domain>Boolean</domain>
            <search unavailable="true"/>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>displayId</id>
            <domain>String</domain>
            <name>Код</name>
        </field>
    </fields>
</query>
<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>metaData</object-id>
    <name>Статус метаданных</name>
    <execution>
        <n2o:java-criteria bean-name="metaDataStatusCriteriaService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.config.criteria.status.MetadataStatusCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>String</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <name>Наименование</name>
            <display/>
            <filters auto-generate="true"/>
        </field>
    </fields>
</query>
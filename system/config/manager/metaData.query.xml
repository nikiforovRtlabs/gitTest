<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>metaData</object-id>
    <name>Метаданные</name>
    <execution>
        <n2o:java-criteria bean-name="metaDataCriteriaService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.config.criteria.metadata.MetaDataCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>code</id>
            <domain>String</domain>
            <name>Идентификатор</name>
            <display/>
            <sorting/>
            <filters>
                <filter type="eq" filter-field="code"/>
            </filters>
        </field>
        <field>
            <id>file</id>
            <domain>String</domain>
            <name>Файл</name>
            <display/>
            <sorting/>
        </field>
        <field>
            <id>metaDataType.id</id>
            <domain>string</domain>
            <filters>
                <filter filter-field="metaDataTypes*.id" type="in">metaDataTypeId</filter>
            </filters>
            <display/>
            <sorting/>
        </field>
        <field>
            <id>metaDataType.name</id>
            <name>Тип</name>
            <domain>string</domain>
            <display/>
            <sorting/>
        </field>
        <field>
            <id>status.id</id>
            <domain>string</domain>
            <filters>
                <filter type="in" filter-field="statuses*.id">statusId</filter>
            </filters>
            <display/>
            <sorting/>
        </field>
        <field>
            <id>status.name</id>
            <name>Статус</name>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>n2oClass</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>invalidMessage</id>
            <domain>String</domain>
            <name>Ошибка валидации</name>
            <display/>
        </field>
        <field>
            <id>invalid</id>
            <name>Невалидные</name>
            <domain>boolean</domain>
            <display/>
            <filters>
                <filter type="eq" filter-field="invalid"/>
            </filters>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <name>Наименование</name>
            <display/>
            <sorting/>
            <filters>
                <filter type="eq" filter-field="name"/>
            </filters>
        </field>
        <field>
            <id>catalog.id</id>
            <name>Каталог</name>
            <domain>String</domain>
            <display/>
            <filters>
                <filter type="eq" filter-field="catalog.id"/>
            </filters>
        </field>
        <field>
            <id>catalog.name</id>
            <domain>String</domain>
            <name>Каталог</name>
            <display/>
        </field>
        <field>
            <id>isSystem</id>
            <domain>Boolean</domain>
            <display/>
        </field>
        <field>
            <id>ancestorStorage</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>displayId</id>
            <domain>String</domain>
            <name>Код</name>
            <display/>
        </field>
        <field>
            <id>localPath</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>storageName</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>warning</id>
            <name> </name>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>configUpdateId</id>
            <domain>string</domain>
            <display/>
        </field>
    </fields>
</query>
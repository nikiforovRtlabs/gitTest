<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>metaData</object-id>
    <name>История</name>
    <execution>
        <n2o:java-criteria bean-name="metaDataHistoryCriteriaService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.config.criteria.audit.history.MetaDataHistoryCriteria"/>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>code</id>
            <name>Хеш-код</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>commit.result</id>
            <name>Текст</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>commit.right</id>
            <name>Предыдущий текст</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>diff</id>
            <name>Изменения</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>localPath</id>
            <name>Путь</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>message</id>
            <name>Сообщение</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>author</id>
            <name>Автор</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
        <field>
            <id>date</id>
            <name>Дата</name>
            <domain>string</domain>
            <filters auto-generate="true"/>
            <display/>
        </field>
    </fields>
</query>
<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>metaData</object-id>
    <name>Каталог метаданных</name>
    <execution>
        <n2o:java-criteria bean-name="codeMergeCriteriaService"
                           criteria-class="net.n2oapp.framework.config.admin.metadata.manager.config.criteria.merge.CodeMergeCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
        </field>
        <field>
            <id>code</id>
            <domain>string</domain>
        </field>
        <field>
            <id>n2oClass</id>
            <domain>string</domain>
        </field>
        <field>
            <id>merge.left</id>
            <domain>String</domain>
            <display>i.left</display>
        </field>
        <field>
            <id>merge.right</id>
            <domain>String</domain>
            <display>i.right</display>
        </field>
        <field>
            <id>merge.result</id>
            <domain>String</domain>
            <display>i.result</display>
        </field>
    </fields>
</query>
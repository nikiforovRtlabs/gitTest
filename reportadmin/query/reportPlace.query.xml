<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <name>Место вызова отчета</name>
    <object-id>reportAdmin</object-id>
    <execution xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
        <n2o:java-criteria bean-name="reportPlaceCriteriaService"
                           criteria-class="ru.kirkazan.rmis.app.report.n2o.place.criteria.place.ReportPlaceCriteria">

        </n2o:java-criteria>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>pageId</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>moduleId</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>spaceId</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>name</id>
            <name>Наименование</name>
            <domain>string</domain>
            <display/>
            <filters>
                <filter type="eq" filter-field="name">name</filter>
            </filters>
        </field>
        <field>
            <id>parentId</id>
            <domain>string</domain>
            <display/>
            <filters>
                <filter type="eq" filter-field="parentId">parentId</filter>
                <filter type="isNull" filter-field="parentIdIsNull">parentIdIsNull</filter>
            </filters>
        </field>
        <field>
            <id>hasChildren</id>
            <domain>boolean</domain>
            <display/>
        </field>
        <field>
            <id>isOpenPage</id>
            <domain>boolean</domain>
            <display/>
        </field>
        <field>
            <id>icon</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>canResolved</id>
            <domain>boolean</domain>
            <display/>
        </field>
    </fields>
</query>


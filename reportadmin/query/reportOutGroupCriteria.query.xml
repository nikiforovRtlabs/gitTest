<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <name>Отчеты</name>
    <object-id>reportGroup</object-id>
    <execution xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
        <n2o:java-criteria bean-name="reportOutGroupCriteriaService"
                           criteria-class="ru.kirkazan.rmis.app.report.n2o.report.criteria.reportOutGroup.ReportOutGroupCriteria">

        </n2o:java-criteria>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
        </field>
        <field>
            <id>name</id>
            <name>Наименование</name>
            <domain>string</domain>
        </field>
        <field>
            <id>fileName</id>
            <name>Файл</name>
            <domain>string</domain>
            <expression>fileName</expression>
        </field>
        <field>
            <id>form_name</id>
            <name>Форма вызова</name>
            <domain>string</domain>
            <expression>formName</expression>
        </field>
        <field>
            <id>form.id</id>
            <name>Форма вызова</name>
            <domain>string</domain>
            <expression>formId</expression>
        </field>
        <field>
            <id>n2oForm.id</id>
            <domain>string</domain>
            <expression>n2oFormId</expression>
        </field>
        <field>
            <id>path</id>
            <name>Путь</name>
            <domain>string</domain>
            <expression>path</expression>
        </field>
        <field>
            <id>inModule_cbx</id>
            <name>Модуль отчетов</name>
            <domain>boolean</domain>
            <expression>isReportModule</expression>
        </field>
        <field>
            <id>is_generated_form</id>
            <name>Генерация формы</name>
            <domain>boolean</domain>
            <expression>isGeneratedForm</expression>
        </field>
        <field>
            <id>group.id</id>
            <domain>integer</domain>
            <expression>groupId</expression>
        </field>
        <field>
            <id>group.name</id>
            <name>Группы</name>
            <domain>string</domain>
            <expression>groupName</expression>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>note</id>
            <name>Описание</name>
            <domain>string</domain>
        </field>
        <field>
            <id>isInvalid</id>
            <domain>boolean</domain>
        </field>
        <field>
            <id>invalidMessage</id>
            <name>Ошибка</name>
            <domain>string</domain>
        </field>
    </fields>
</query>


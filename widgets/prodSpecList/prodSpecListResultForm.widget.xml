<form xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-2.0" src="inventory/custom/specListResultForm" customize="css,jsp">
    <name> </name>
    <query-id>productionSpecListResult</query-id>
    <default-values-query-id>productionSpecListResult</default-values-query-id>
    <edit model="default"/>
    <fs:field-set>
        <fs:row>
            <ctrl:output-text id="specCount" label="Количество спецификаций:" label-style="width: 170px" control-style="width:50px"
                              dependency-condition="!(specCount > 10)"/>
            <ctrl:output-text id="amountSum" label="Сумма по спецификациям:" label-style="width: 170px"/>
        </fs:row>
    </fs:field-set>
</form>
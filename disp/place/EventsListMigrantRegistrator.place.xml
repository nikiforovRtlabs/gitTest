<place xmlns="http://n2oapp.net/framework/config/schema/report-n2o-place-1.0">
    <containers>
        <container id="migrantList" report-menu-item-id="sub_reports">
            <reports>
                <report-form form-id="monitoring_migrant" format="pdf" label="Мониторинг проведения медицинских осмотров ИГ и ЛГ за период"/>
                <report-url code="n2o/conclusion_migrant.rptdesign" format="pdf" label="Медицинское заключение о состоянии здоровья иностранного гражданина">
                    <params>
                        <param name="case_id" ref="caseId"/>
                        <param name="patient_id" ref="indivId"/>
                    </params>
                </report-url>
                <report-url code="n2o/cert_hiv_migrant.rptdesign" format="pdf" label="Справка об обследовании на антитела к вирусу иммунодефицита человека (ВИЧ)">
                    <params>
                        <param name="case_id" ref="caseId"/>
                        <param name="patient_id" ref="indivId"/>
                    </params>
                </report-url>
                <report-url code="n2o/referral_on_blood.rptdesign" format="pdf" label="Направления на кровь">
                    <params>
                        <param name="case_id" ref="caseId"/>
                        <param name="patient_id" ref="indivId"/>
                    </params>
                </report-url>
                <report-url code="n2o/agreement_fluo.rptdesign" format="pdf" label="Информированное добровольное согласие на проведение флюорографического обследования">
                    <params>
                        <param name="case_id" ref="caseId"/>
                        <param name="patient_id" ref="indivId"/>
                    </params>
                </report-url>
            </reports>
        </container>
    </containers>
</place>
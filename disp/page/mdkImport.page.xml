                        <ctrl:file-upload id="mdkImportXml" upload-url="/backend/mdk-import-rest/mdk-import-rest/file" label="Файл для импорта" types=".xml"/>
                    </fs:field-set>
                </wgt:edit-form>
            </container>
        </region>
    </regions>

</page>

<?xml version='1.0' encoding='UTF-8'?>
<query
        xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
        xmlns="http://n2oapp.net/framework/config/schema/query-3.0"
        >
    <name>Диагнозы</name>
    <object-id>disprCardDiagnosisGroupEdit</object-id>
    <execution>
        <n2o:rest>
            <n2o:query>
                ${n2o.backend.path}/dispensary/vop/diagnosis/getDataPatientForm
            </n2o:query>
        </n2o:rest>
    </execution>

    <fields>
        <field>
            <id>diagnosisGroupId</id>
            <domain>Integer</domain>
            <display/>
            <filters auto-generate="true">
                <filter type="eq" filter-field="diagnosisGroupId"/>
            </filters>
        </field>
        <field>
            <id>eventPatientId</id>
            <domain>Integer</domain>
            <display/>
            <filters auto-generate="true">
                <filter type="eq" filter-field="eventPatientId"/>
            </filters>
        </field>
        <field>
            <id>diagnosisGroupName</id>
            <domain>String</domain>
            <display/>
            <name></name>
        </field>
        <field>
            <id>diagnosis.id</id>
            <domain>Integer</domain>
            <display>diagnosisId</display>
        </field>
        <field>
            <id>diagnosis.name</id>
            <domain>String</domain>
            <display>diagnosisName</display>
        </field>
        <field>
            <id>diagnosisNo</id>
            <domain>string</domain>
            <display/>
        </field>
        <field>
            <id>preliminaryDate</id>
            <domain>Date</domain>
            <display/>
            <name>Подозрение</name>
        </field>
        <field>
            <id>firstDate</id>
            <domain>Date</domain>
            <display/>
            <name>Впервые выявлено</name>
        </field>
        <field>
            <id>establishmentDate</id>
            <domain>Date</domain>
            <display/>
            <name>Выявлено</name>
        </field>
        <field>
            <id>overviewColor</id>
            <domain>Integer</domain>
            <display/>
        </field>
        <field>
            <id>reason</id>
            <domain>String</domain>
            <display/>
        </field>
    </fields>
</query>
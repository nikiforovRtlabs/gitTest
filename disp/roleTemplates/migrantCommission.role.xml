    <group>Мероприятия</group>
    <section id="domainObjects">
        <element id="migrCards">
            <permission id="readCards"/>
            <permission id="readServices"/>
            <permission id="EMKcommission"/>
            <permission id="Reportscommission"/>
            <permission id="editCardcommission"/>
            <permission id="viewCloseCardcommission"/>

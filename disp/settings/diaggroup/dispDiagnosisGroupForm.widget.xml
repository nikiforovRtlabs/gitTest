<form xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-1.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <query-id>dispDiagnosisGroupList</query-id>

    <fs:field-set>
        <ctrl:input-text id="no" label="Номер строки">
            <ctrl:validations>
                <ctrl:validation ref-id="assertNo"/>
            </ctrl:validations>
        </ctrl:input-text>
        <ctrl:input-text id="name" label="Наименование классов и отдельных болезней" required="true"/>
        <ctrl:input-text id="code" label="Коды МКБ-10" required="true"/>
    </fs:field-set>
</form>

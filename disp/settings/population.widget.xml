<?xml version="1.0" encoding="UTF-8"?>
<wgt:form xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
          xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
          xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <wgt:name>Численность населения</wgt:name>
    <wgt:query-id>population</wgt:query-id>
    <wgt:default-values-query-id>populationDefault</wgt:default-values-query-id>
    <fs:field-set field-label-location="top">
        <fs:row>
            <ctrl:input-text id="org_name" label="Медицинская организация" readonly="true"/>
            <ctrl:hidden id="org_id"/>
            <ctrl:date-time id="year" label="Год" css-class="n2o-label-4 n2o-control-4" default-value="beginYear()"/>
        </fs:row>
    </fs:field-set>
    <fs:field-set header="line" label="Численность населения на начало года">
        <fs:row>
            <ctrl:output-text id="label21_36" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label39_60" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label60" label="" css-class="n2o-label-3 n2o-control-4"/>
        </fs:row>
        <fs:row>
            <ctrl:input-text id="m21_36_begin" label="М" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="m39_60_begin" label="М" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="m60_begin" label="М" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
        <fs:row>
            <ctrl:input-text id="f21_36_begin" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="f39_60_begin" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="f60_begin" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
    </fs:field-set>
    <fs:field-set header="line" label="Подлежит диспансеризации по плану текущего года">
        <fs:row>
            <ctrl:output-text id="label21_36" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label39_60" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label60" label="" css-class="n2o-label-3 n2o-control-4"/>
        </fs:row>
        <fs:row>
            <ctrl:input-text id="m21_36_disp" label="М" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="m39_60_disp" label="М" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="m60_disp" label="М" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
        <fs:row>
            <ctrl:input-text id="f21_36_disp" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="f39_60_disp" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="f60_disp" label="Ж" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
        <fs:row>
            <ctrl:output-text id="label0-4" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label5-9" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label10-14" label="" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:output-text id="label15-17" label="" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
        <fs:row>
            <ctrl:input-text id="ch0_4_disp" label="Дети" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="ch5_9_disp" label="Дети" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="ch10_14_disp" label="Дети" css-class="n2o-label-3 n2o-control-3"/>
            <ctrl:input-text id="ch15_17_disp" label="Дети" css-class="n2o-label-3 n2o-control-3"/>
        </fs:row>
    </fs:field-set>
    <wgt:action-menu>
        <wgt:menu-item id="update" label="Редактировать" context="true" type="textAndIcon">
            <wgt:edit>
                <wgt:invoke-action action-id="update"/>
            </wgt:edit>
        </wgt:menu-item>
    </wgt:action-menu>
</wgt:form>
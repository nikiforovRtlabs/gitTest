<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-1.0">
<id>customSetPlannedDate</id>
<name>Вы уверены?</name>
<object-id>eventPatients</object-id>
<layout>n2o/layout/single</layout>
<containers>
    <container id="main" place="single">
        <wgt:form detail-field-id="id" master-field-id="id">
            <wgt:id>customSetPlannedDate</wgt:id>
            <wgt:query-id>eventPatients</wgt:query-id>

            <wgt:field-set>
                <ctrl:hidden id="eventId" domain="integer"/>
            </wgt:field-set>

            <wgt:action-menu>
                <wgt:menu-item label="Изменить" id="update">
                    <wgt:go-edit action-id="update" />
                </wgt:menu-item>
            </wgt:action-menu>
        </wgt:form>
    </container>
</containers>
</page>
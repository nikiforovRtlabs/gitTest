<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>paymentControl</object-id>
    <execution>
        <n2o:sql>
            <n2o:items-query>select :select</n2o:items-query>
            <n2o:count-query>select 1</n2o:count-query>
        </n2o:sql>
    </execution>
    <fields>
        <field>
            <id>patientId</id>
            <domain>integer</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>patient.id</id>
            <domain>integer</domain>
            <display>:patientId</display>
        </field>

        <field>
            <id>orgId</id>
            <domain>integer</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>orgName</id>
            <domain>string</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>org.name</id>
            <domain>string</domain>
            <display>:orgName</display>
        </field>
        <field>
            <id>org.id</id>
            <domain>integer</domain>
            <display>:orgId</display>
        </field>


        <field>
            <id>fundingId</id>
            <domain>integer</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>funding.id</id>
            <domain>integer</domain>
            <display>:fundingId</display>
        </field>

        <field>
            <id>contractId</id>
            <domain>integer</domain>
            <display unavailable="true"/>
        </field>
        <field>
            <id>contract.id</id>
            <domain>integer</domain>
            <display>:contractId</display>
        </field>
    </fields>
</query>


<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">

    <object-id>receive_record</object-id>
    <name>receive_record_defaults</name>
    <execution>
        <n2o:sql>
            <n2o:items-query>
                select :select
            </n2o:items-query>
            <n2o:count-query>
                select 1
            </n2o:count-query>
        </n2o:sql>


    </execution>

    <fields>
        <field>
            <id>restQuantity</id>
            <domain>integer</domain>
            <expression>(SELECT sickdoc.counting_rest_count(:orgId))</expression>
        </field>
        <field>
            <id>resultQuantity</id>
            <domain>integer</domain>
            <expression>(SELECT sickdoc.counting_rest_count(:orgId))</expression>
        </field>
    </fields>
</query>

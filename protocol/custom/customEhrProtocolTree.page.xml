<?xml version='1.0' encoding='UTF-8'?>
<page xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns="http://n2oapp.net/framework/config/schema/page-1.0">
    <object-id>customEhrProtocol</object-id>
    <layout>n2o/layout/single</layout>
    <name>Выбор шаблона</name>
    <properties>
        <property key="help-url" value="https://wiki.kirkazan.ru/display/N2O/N2O.Documentation.Widget.Tree"/>
    </properties>
    <containers>
        <container id="customProtocolWithGroup" place="single">
            <wgt:tree>
                <wgt:name>Client</wgt:name>
                <wgt:query-id>customEhrProtocolTree</wgt:query-id>
                <wgt:ajax>false</wgt:ajax>
                <wgt:inheritance-nodes parent-field-id="parentId" label-field-id="name" has-children-field-id="hasChildren" can-resolved-field-id="resolved"/>
            </wgt:tree>

        </container>
    </containers>

</page>
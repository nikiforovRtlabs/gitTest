<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>clinicPrescription</object-id>
    <layout>n2o/layout/single</layout>
    <name>Отмена назначения</name>

    <regions>
        <region place="single" type="none">
            <container id="main">
                <wgt:form>
                    <wgt:query-id>clinicPrescriptionList</wgt:query-id>
                    <wgt:name>Отменить</wgt:name>
                    <fs:field-set>
                        <ctrl:date-time id="cancelDate" label="Дата отмены" required="true"/>
                        <ctrl:input-text id="cancelReason" label="Причина отмены" required="true"/>
                    </fs:field-set>
                </wgt:form>
            </container>
        </region>
    </regions>

</page>


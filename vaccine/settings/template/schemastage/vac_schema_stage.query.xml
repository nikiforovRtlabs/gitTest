<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>vac_schema_stage</object-id>
    <execution>
        <n2o:rest>
            <n2o:query>
                ${n2o.backend.path}/vaccine/schema-stages
            </n2o:query>
            <n2o:query-by-id>
                ${n2o.backend.path}/vaccine/schema-stages/{id}
            </n2o:query-by-id>
            <n2o:response-mapping collection="list" count="size"/>
            <n2o:date-format>dd.MM.yyy</n2o:date-format>
        </n2o:rest>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <expression>id</expression>
            <search>:id</search>
        </field>
        <field>
            <id>schemaId</id>
            <domain>integer</domain>
            <expression>schemaId</expression>
            <search>:expression=:schemaId</search>
        </field>
        <field>
            <id>prevStage.id</id>
            <domain>integer</domain>
            <expression>prevStage.id</expression>
        </field>
        <field>
            <id>prevStage.number.id</id>
            <domain>integer</domain>
            <expression>prevStage.number.id</expression>
        </field>
        <field>
            <id>prevStage.number.label</id>
            <domain>string</domain>
            <name>Предыдущий этап</name>
            <expression>prevStage.number.label</expression>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>disease.id</id>
            <domain>integer</domain>
            <display>disease.id</display>
        </field>
        <field>
            <id>disease.label</id>
            <domain>integer</domain>
            <name>Инфекция</name>
            <display>disease.label</display>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>number.id</id>
            <domain>integer</domain>
            <expression>number.id</expression>
        </field>
        <field>
            <id>number.label</id>
            <domain>string</domain>
            <name>Этап</name>
            <expression>number.label</expression>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>rules</id>
            <domain>string</domain>
            <name>Правило</name>
            <expression>rules</expression>
            <sorting unavailable="true"/>
        </field>
        <field>
            <id>age.min.years</id>
            <domain>integer</domain>
            <expression>age.min.years</expression>
        </field>
        <field>
            <id>age.min.months</id>
            <domain>integer</domain>
            <expression>age.min.months</expression>
        </field>
        <field>
            <id>age.min.days</id>
            <domain>integer</domain>
            <expression>age.min.days</expression>
        </field>
        <field>
            <id>age.max.years</id>
            <domain>integer</domain>
            <expression>age.max.years</expression>
        </field>
        <field>
            <id>age.max.months</id>
            <domain>integer</domain>
            <expression>age.max.months</expression>
        </field>
        <field>
            <id>age.max.days</id>
            <domain>integer</domain>
            <expression>age.max.days</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.min.years</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.min.years</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.min.months</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.min.months</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.min.days</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.min.days</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.max.years</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.max.years</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.max.months</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.max.months</expression>
        </field>
        <field>
            <id>periodAfterPrevStage.max.days</id>
            <domain>integer</domain>
            <expression>periodAfterPrevStage.max.days</expression>
        </field>
        <field>
            <id>periodical.min.years</id>
            <domain>integer</domain>
            <expression>periodical.min.years</expression>
        </field>
        <field>
            <id>periodical.min.months</id>
            <domain>integer</domain>
            <expression>periodical.min.months</expression>
        </field>
        <field>
            <id>periodical.min.days</id>
            <domain>integer</domain>
            <expression>periodical.min.days</expression>
        </field>
        <field>
            <id>isFirstStage</id>
            <domain>boolean</domain>
            <expression>isFirstStage</expression>
        </field>
    </fields>
</query>

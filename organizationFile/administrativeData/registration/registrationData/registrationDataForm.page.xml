<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:rc="http://atria.cz/app/config/schema/rmis-control-1.0">
    <object-id>registrationData</object-id>
    <layout>n2o/layout/single</layout>
    <regions>
        <region place="single">
            <container id="main">
                <wgt:form detail-field-id="id" master-field-id="id">
                    <wgt:query-id>registrationData</wgt:query-id>
                    <wgt:name>Регистрационные данные</wgt:name>
                    <fs:field-set>
                        <ctrl:input-text id="issuer_name" label="Наименование органа" required="true"/>
                        <ctrl:input-text id="reg_doc" label="Документ о регистрации" required="true"/>
                        <ctrl:date-time id="reg_date" label="Дата регистрации" required="true"/>
                        <ctrl:input-text id="reg_number" label="Регистрационный номер" required="true"/>
                    </fs:field-set>
                </wgt:form>
            </container>
        </region>
    </regions>
</page>
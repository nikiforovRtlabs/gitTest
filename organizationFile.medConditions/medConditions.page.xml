<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>medConditions</object-id>
    <layout>n2o/layout/single</layout>
    <name>Условия размещения</name>

    <regions>
        <region place="single" type="pills">
            <container id="accomodationConditions">
                <wgt:table ref-id="accomodationConditionsList"/>
            </container>

            <container id="unfrastructureObjects">
                <wgt:table ref-id="unfrastructureObjectsList"/>
            </container>

            <container id="medicalServices">
                <wgt:table ref-id="medicalServicesList"/>
            </container>

            <container id="medicalTechnologies">
                <wgt:table ref-id="medicalTechnologiesList"/>
            </container>

            <container id="nutrition">
                <wgt:table ref-id="nutritionList"/>
            </container>

        </region>
    </regions>
</page>
<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>defFormList</object-id>
    <name>Список форм</name>

    <execution>
        <n2o:java-criteria bean-name="form"
                           criteria-class="ru.kirkazan.rmis.user.defaults.admin.n2o.criteria.form.WidgetCriteria"/>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>Integer</domain>
            <display/>
        </field>
        <field>
            <id>placeId</id>
            <domain>String</domain>
            <filters>
                <filter type="eq" filter-field="placeId"/>
            </filters>
        </field>
        <field>
            <id>name</id>
            <domain>String</domain>
            <filters>
                <filter type="eq" filter-field="name"/>
            </filters>
            <display/>
        </field>
        <field>
            <id>type</id>
            <domain>String</domain>
            <filters>
                <filter type="eq" filter-field="type"/>
            </filters>
        </field>
        <field>
            <id>widgetId</id>
            <domain>String</domain>
            <display/>
        </field>
        <field>
            <id>pageId</id>
            <domain>String</domain>
            <display/>
        </field>
        <field>
            <id>containerId</id>
            <domain>String</domain>
            <display/>
        </field>
    </fields>
</query>

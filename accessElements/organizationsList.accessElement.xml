<element xmlns="http://atria.cz/sec/config/schema/element-1.0"
         xmlns:n2o="http://atria.cz/sec/config/schema/n2o-permission-1.0"
         name="Медицинские организации">

    <permission id="updateOrganizationsList" name="Редактирование организаций" group-id="update">
        <n2o:object-access object-id="organizationsList"  actions="*"/>
    </permission>

    <permission id="readOrganizationsList" name="Просмотр организаций" group-id="read">
        <n2o:object-access object-id="organizationsList"/>
    </permission>

</element>
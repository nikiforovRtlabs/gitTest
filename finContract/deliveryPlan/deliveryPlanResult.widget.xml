<form xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
       xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/n2o-widget-2.0" src="inventory/custom/finContract/deliveryPlan/deliveryPlanResult" customize="css,jsp">
    <name> </name>
    <query-id>deliveryPlanResult</query-id>
    <default-values-query-id>deliveryPlanResult</default-values-query-id>
    <edit model="default"/>
    <fs:field-set>
        <fs:row>
            <ctrl:output-text id="countOfArrivalInvoices" label="Количество документов:" label-style="width: 170px" control-style="width:50px"
                              dependency-condition="!(countOfArrivalInvoices > 10)"/>
            <ctrl:output-text id="amountSum" css-class="n2o-label-5 n2o-control-3"/>
            <ctrl:output-text id="executedAmountSum" css-class="n2o-label-7 n2o-control-5"/>
        </fs:row>
    </fs:field-set>
</form>
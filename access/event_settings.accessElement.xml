<element xmlns="http://atria.cz/sec/config/schema/element-1.0"
         xmlns:n2o="http://atria.cz/sec/config/schema/n2o-permission-1.0"
         id="event_settings" name="Настройки критериев с типом событие">
    <!--Просмотр-->
    <permission id="readJournal" name="Просмотр" group-id="read">
        <n2o:object-access object-id="event_settings"/>
    </permission>

    <!--Создание-->
    <permission id="createEvent" name="Создание" group-id="create">
        <n2o:object-access object-id="event_settings" actions="create"/>
    </permission>

    <!--Редактирование-->
    <permission id="editEvent" name="Редактирование" group-id="update">
        <n2o:object-access object-id="event_settings" actions="update"/>
    </permission>

    <!--Удаление-->
    <permission id="deleteEvent" name="Удаление" group-id="delete">
        <n2o:object-access object-id="event_settings" actions="delete"/>
    </permission>


</element>
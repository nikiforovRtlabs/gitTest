<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-2.0">
    <object-id>monitoring</object-id>
    <name>monitoring_refresh_info</name>
    <execution>
        <n2o:sql>
            <n2o:items-query>select :select
                from motherhood.mh_treatment_plan tp
                where :where
                order by :order
            </n2o:items-query>
            <n2o:count-query>select count(tp)
                from motherhood.mh_treatment_plan tp
                where :where
            </n2o:count-query>
            <n2o:alias>tp</n2o:alias>
        </n2o:sql>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
        </field>

        <field>
            <id>refreshDate</id>
            <domain>integer</domain>
            <expression>tp.refresh_dt</expression>
        </field>
    </fields>
</query>

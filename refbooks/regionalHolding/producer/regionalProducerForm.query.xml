<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <object-id>regionalProducer</object-id>
    <execution>
        <n2o:sql>
            <n2o:items-query>select :select
                from inventory.common_holding_producer hp
                left outer join pim_organization org on org.id=hp.org_id
                where :where
                order by :order
            </n2o:items-query>
            <n2o:count-query>select 1
            </n2o:count-query>
            <n2o:alias>hp</n2o:alias>
        </n2o:sql>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
        </field>
        <field>
            <id>modifId</id>
            <domain>integer</domain>
            <expression>hp.common_hold_modif_id</expression>
        </field>
        <field>
            <id>org.id</id>
            <domain>integer</domain>
            <expression>hp.org_id</expression>
        </field>
        <field>
            <id>org.name</id>
            <domain>string</domain>
            <name>Производитель</name>
            <expression>org.short_name</expression>
        </field>
        <field>
            <id>codeRls</id>
            <name>Код РЛС</name>
            <domain>string</domain>
            <expression>hp.code_rls</expression>
        </field>
        <field>
            <id>ean</id>
            <name>Штрих-код (EAN13)</name>
            <domain>string</domain>
            <expression>hp.ean13</expression>
        </field>
        <field>
            <id>regCertNumber</id>
            <domain>string</domain>
            <name>Рег. удостоверение</name>
            <expression>hp.reg_cert_number</expression>
        </field>
        <field>
            <id>certNum</id>
            <domain>string</domain>
            <name>Сертификат</name>
            <expression>hp.cert_num</expression>
        </field>
        <field>
            <id>regCertDateStart</id>
            <domain>date</domain>
            <name>Дата начала</name>
            <expression>hp.reg_cert_start_dt</expression>
        </field>
        <field>
            <id>regCertDateEnd</id>
            <domain>date</domain>
            <name>Дата окончания</name>
            <expression>hp.reg_cert_end_dt</expression>
        </field>
        <field>
            <id>limitPrice</id>
            <domain>string</domain>
            <name>Предельная цена</name>
            <expression>hp.limit_price</expression>
        </field>
        <field>
            <id>priceDate</id>
            <domain>date</domain>
            <name>Дата цены</name>
            <expression>hp.limit_price_dt</expression>
        </field>
        <field>
            <id>decisionNum</id>
            <domain>string</domain>
            <name>№ решения</name>
            <expression>hp.decision_number</expression>
        </field>
        <field>
            <id>zdravCode</id>
            <name>Код Росздравнадзора</name>
            <domain>string</domain>
            <expression>hp.ross_zdr_code</expression>
        </field>
        <field>
            <id>isActive</id>
            <name>Активный</name>
            <domain>boolean</domain>
            <expression>hp.is_active</expression>
        </field>
    </fields>
</query>
<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0">
    <name>Первый вопрос</name>
    <object-id>ambulance_answer</object-id>
    <execution>
        <n2o:sql>
            <n2o:items-query>
                select :select
                from amb.md_ambulance_question aq
                where aq.is_first_question is true and :where
            </n2o:items-query>
            <n2o:count-query>select count(*)
                from amb.md_ambulance_question aq
                where aq.is_first_question is true and :where
            </n2o:count-query>
            <n2o:alias>aq</n2o:alias>
        </n2o:sql>
    </execution>
    <fields>
        <field>
            <id>question_id</id>
            <domain>integer</domain>
            <name>Идентификатор</name>
            <expression>aq.id</expression>
        </field>
        <field>
            <id>question_text</id>
            <domain>integer</domain>
            <name>Идентификатор</name>
            <expression>aq.question_text</expression>
        </field>
    </fields>
</query>
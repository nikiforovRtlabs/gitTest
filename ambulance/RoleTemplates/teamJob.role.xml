<role xmlns="http://atria.cz/sec/config/schema/role-1.0">
    <name>Наряды</name>
    <group>Скорая помощь</group>
    <section id="domainObjects">
        <element id="brg">
            <permission id="read"/>
            <permission id="create"/>
            <permission id="update"/>
            <permission id="delete"/>
        </element>
        <element id="settings">
            <permission id="readChange"/>
        </element>
    </section>
    <section id="modules" access="denied">
        <element id="modules.amb" access="allowed"/>
    </section>
</role>
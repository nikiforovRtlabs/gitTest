<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-1.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-1.0">
    <object-id>employee</object-id>
    <layout>n2o/layout/single</layout>
    <containers>
        <container id="place" place="single">
            <wgt:table>
                <wgt:query-id>employee</wgt:query-id>
                <wgt:columns>
                    <wgt:column column-field-id="id"/>
                    <wgt:column column-field-id="station.name"/>
                    <wgt:column column-field-id="full_name"/>
                    <wgt:column column-field-id="name"/>
                    <wgt:column column-field-id="individuals.birthday"/>
                    <wgt:column column-field-id="age"/>
                    <wgt:column column-field-id="gender.name"/>
                </wgt:columns>
            </wgt:table>
        </container>
    </containers>
</page>
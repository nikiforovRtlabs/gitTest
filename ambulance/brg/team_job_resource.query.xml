            <domain>string</domain>
            <name>Подстанция</name>
            <expression>pd.name</expression>
            <search unavailable="true"/>
        </field>

        <field>
            <id>team_kind.id</id>
            <domain>integer</domain>
            <expression>tk.id</expression>
        </field>
        <field>

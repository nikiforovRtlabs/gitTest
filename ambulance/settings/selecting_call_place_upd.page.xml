<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0">
    <name>Карточка настройки посыла по поводу:место вызова</name>
    <object-id>selecting_call_place</object-id>
    <layout>n2o/layout/single</layout>
    <containers>
        <container id="single" place="single">
            <wgt:form>
                <wgt:name>Карточка настройки места вызова</wgt:name>
                <wgt:query-id>selecting_call_place</wgt:query-id>
                <fs:field-set>
                    <ctrl:classifier id="callPlace" label="Место вызова" search-are-you-type="true" required="true">
                        <ctrl:query query-id="call_place" value-field-id="id" label-field-id="name"/>
                    </ctrl:classifier>
                </fs:field-set>
            </wgt:form>
        </container>
    </containers>
</page>
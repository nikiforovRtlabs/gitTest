<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-2.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <!--<id>take_birth_upd</id>-->
    <name>Место принятия родов</name>
    <object-id>take_birth</object-id>
    <layout>n2o/layout/single</layout>
    <containers>
        <container id="single" place="single">
            <wgt:form>
                <wgt:name>Место принятия родов</wgt:name>
                <wgt:query-id>take_birth</wgt:query-id>
                <fs:field-set>
                    <ctrl:input-text id="id" readonly="true" length="10"/>
                    <ctrl:input-text id="code" required="true">
                        <ctrl:validations>
                            <ctrl:validation ref-id="take_birthCodeUnique"/>
                        </ctrl:validations>
                    </ctrl:input-text>
                    <ctrl:input-text id="name" required="true">
                        <ctrl:validations>
                            <ctrl:validation ref-id="take_birthNameUnique"/>
                        </ctrl:validations>
                    </ctrl:input-text>
                    <ctrl:input-text id="e_code">
                        <ctrl:validations>
                            <ctrl:validation ref-id="take_birthEcodeUnique"/>
                        </ctrl:validations>
                    </ctrl:input-text>
                </fs:field-set>
            </wgt:form>
        </container>
    </containers>
</page>